#include <iostream>
using namespace std;
class Rectangle{
    int length;
    int breadth;
    public:
    Rectangle(){
        cout<<"Please enter the length: ";
        cin>>length;
        cout<<"Please enter the breadth: ";
        cin>>breadth;
    }
    void display(){
        cout<<length*breadth;
    }
};
int main() {
    Rectangle rectangle;
    rectangle.display();
    return 0;
}
